{
    "name": "inovartecnologia/object-values",
    "description": "Object Values em PHP para representar valores usuais no Brasil.",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "rodrigo",
            "email": "rodrigo@inovartecnologia.tech"
        }
    ],
    "require": {
        "php": "^8.1|^8.2",
    },
    "autoload": {
        "psr-4": {
            "InovarTecnologia\\ObjectValues\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "pestphp/pest": "^2.14",
        "phpstan/phpstan": "^1.10"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
